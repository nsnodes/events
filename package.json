{
  "name": "nsnodes-events",
  "version": "1.0.0",
  "description": "Event aggregation platform - scraping and normalizing events from multiple sources",
  "type": "module",
  "scripts": {
    "probe": "node tools/probes/run-all-tests.js",
    "probe:luma": "node tools/probes/luma-probe.js",
    "probe:sola": "node tools/probes/sola-probe.js",
    "luma:cities": "node packages/sources/luma/scripts/run-cities.js",
    "luma:ical-urls": "node packages/sources/luma/scripts/run-ical-urls.js",
    "luma:events": "node packages/sources/luma/scripts/run-events.js",
    "sola:cities": "node packages/sources/sola/scripts/run-cities.js",
    "sola:city-details": "node packages/sources/sola/scripts/run-city-details.js",
    "sola:ical-urls": "node packages/sources/sola/scripts/run-ical-urls.js",
    "sola:events": "node packages/sources/sola/scripts/run-events.js",
    "schedule:daily": "tsx apps/cli/run.ts schedule:daily",
    "schedule:weekly": "tsx apps/cli/run.ts schedule:weekly",
    "schedule:polling": "tsx apps/cli/run.ts schedule:polling",
    "task": "tsx apps/cli/run.ts task",
    "list": "tsx apps/cli/run.ts list"
  },
  "keywords": [
    "events",
    "scraping",
    "playwright",
    "luma",
    "sola",
    "aggregation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "geo-tz": "^8.1.4",
    "glob": "^11.0.0",
    "i18n-iso-countries": "^7.14.0",
    "node-geocoder": "^4.4.1",
    "node-ical": "^0.22.0",
    "playwright": "^1.48.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "supabase": "^1.226.4",
    "tsx": "^4.20.6"
  },
  "scripts-notes": {
    "db:migrate": "Run after 'supabase link': supabase db push"
  }
}
